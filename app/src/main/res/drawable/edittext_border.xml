<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Mengatur warna latar belakang EditText (bisa dihapus jika tidak diperlukan) -->
    <solid android:color="#FFFFFF" />

    <!-- Mengatur border -->
    <stroke
        android:width="1dp"
        android:color="#808080" /> <!-- Ubah warna sesuai kebutuhan -->

    <!-- Mengatur radius sudut (opsional) -->
    <corners android:radius="10dp" />
</shape>
